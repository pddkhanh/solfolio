{
  "name": "solfolio-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "jest --watchAll=false --forceExit --detectOpenHandles --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=50%",
    "test:coverage": "jest --coverage --watchAll=false --forceExit --maxWorkers=50%",
    "test:e2e": "playwright test --reporter=list",
    "test:e2e:headless": "playwright test --reporter=list --workers=1",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug --headed",
    "test:e2e:testmode": "NEXT_PUBLIC_E2E_TEST_MODE=true playwright test --reporter=list",
    "test:e2e:testmode:ui": "NEXT_PUBLIC_E2E_TEST_MODE=true playwright test --ui",
    "test:e2e:testmode:debug": "NEXT_PUBLIC_E2E_TEST_MODE=true playwright test --debug --headed",
    "dev:testmode": "NEXT_PUBLIC_E2E_TEST_MODE=true next dev"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "@types/socket.io-client": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "google-protobuf": "^4.0.0",
    "grpc-web": "^1.5.0",
    "lucide-react": "^0.541.0",
    "next": "^15.1.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@protobuf-ts/grpc-transport": "^2.11.1",
    "@protobuf-ts/plugin": "^2.11.1",
    "@protobuf-ts/runtime": "^2.11.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.17.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.3",
    "eventemitter3": "^5.0.1",
    "grpc-tools": "^1.13.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "pino-pretty": "^13.1.1",
    "playwright": "^1.55.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.3"
  }
